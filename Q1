package mypackage;


public class Person {
	 public static void main(String[] args) {
	        Person person = new Person("Surya", 24);
	        System.out.println("Name: " + person.getName());
	        System.out.println("Age: " + person.getAge());
	 }
    private String name;
    private int age;

    public Person(String name, int age) {
        this.name = name;
        this.age = age;
    }

    public String getName() {
        return name;
    }

    public int getAge() {
        return age;
    }
    
}
